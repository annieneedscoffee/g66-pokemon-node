<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Trainers</title>
  </head>
  <body>
    <h1>Trainers</h1>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Trainer ID</th>
          <th>Show</th>
        </tr>
      </thead>
      <tbody>
      <%  for(let i=0;i<trainers.length;i++){ %>
        <tr>
          <td><%= trainers[i].name %></td>
          <td><%= trainers[i].id %></td>
          <td><a href="/show/trainers/<%=trainers.id%>">Show</a></td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </body>
</html>
